
Run {
  top [main+] = ‛«main»’
  main [spaces1 patternLR spaces2] = ‛«spaces1»«patternLR»«spaces2»’
  bracket [c] = ‛«c»’
  char [c] = ‛«c»’

  matchLR_atLeastOneSpace [sp+] = ‛«sp»’
  matchLR_pattern [lr] = ‛«lr»’
  matchLR_char [char+] = ‛«char»’

  item [spaces matchLR ] = ‛«spaces»«matchLR»’
  literal [c] =  ‛«c»’
  
  patternLR [ ignore1  p  d  s  m  ignore2 ] = 
‛⟨MessageWithDebug «p» «d» «s» «m»⟩’

}

