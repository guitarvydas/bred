Bred {
  main = spaces firstPattern spaces secondPattern spaces

  firstPattern = "‛" firstitem+ "’"
  firstitem =
    | "«" name "»" -- variable
    | space -- space
    | (~"«" ~"»" ~"‛" ~"’" ~space any)+ -- characters
  secondPattern = "‛" seconditem+ "’"
  seconditem =
    | "«" name "»" -- variable
    | space -- space
    | (~"«" ~"»" ~"‛" ~"’" ~space any)+ -- characters

  name = nameFirst nameRest*
  nameFirst = letter
  nameRest = alnum
}
